{
  "name": "@sensate/gateway-discord",
  "version": "0.0.1",
  "description": "Discord gateway for the sensate bot framework.",
  "main": "lib/index.js",
  "scripts": {
    "build": "make build",
    "test": "make test",
    "coverage": "make test-cov",
    "coverage-level": "make test-cov-level",
    "lint": "make lint",
    "flow": "make flow",
    "postpublish": "greenkeeper-postpublish",
    "prepublish": "npm run build"
  },
  "repository": "sensate-framework/gateway-discord",
  "bugs": "https://github.com/sensate-framework/gateway-discord/issues",
  "homepage": "https://github.com/sensate-framework/gateway-discord#readme",
  "author": "Nico Hämäläinen <nico@sizeof.io>",
  "keywords": [
    "sensate",
    "discord",
    "bot"
  ],
  "license": "MIT",
  "pre-commit": {
    "silent": true,
    "run": [
      "lint",
      "flow",
      "test",
      "coverage"
    ]
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "cache": true,
    "check-coverage": true
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "mocha": true
    },
    "plugins": [
      "flowtype"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint-plugin-flowtype": "^2.11.0",
    "flow-bin": "^0.43.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.0.0-beta.5"
  },
  "dependencies": {
    "@sensate/sensate": "*",
    "discord.js": "^8.1.0"
  }
}
